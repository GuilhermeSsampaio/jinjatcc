import { useContext, useEffect } from "react";
import { Navigate, Outlet } from "react-router-dom";
import ContextoUsuario from "../contextos/contexto_usuario";
import MenuLateral from "../componentes/menu_lateral";

export default function RotasUsuariosLogados() {
  const { usuarioLogado } = useContext(ContextoUsuario);

  if (usuarioLogado?.perfil)
    return (
      <MenuLateral>
        <Outlet />
      </MenuLateral>
    );
  else return <Navigate to="/" />;
}
